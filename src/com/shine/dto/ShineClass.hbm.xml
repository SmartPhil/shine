<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shine.dto.ShineClass" table="class">
		<id name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="level" column="level"></property>
		<property name="classCode" column="classCode"></property>
		<property name="beginDate" column="beginDate"></property>
		<property name="endDate" column="endDate"></property>
		<property name="beginWeek" column="beginWeek"></property>
		<property name="beginTime" column="beginTime"></property>
		<property name="foreignTeacher" column="foreignTeacher"></property>
		<property name="chinaTeacher" column="chinaTeacher"></property>
		<property name="classManager" column="classManager"></property>
		<property name="fee" column="fee"></property>
	</class>
</hibernate-mapping>