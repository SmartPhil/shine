<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shine.dto.Application" table="application">
		<id name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="classCode" column="classCode"></property>
		<property name="applyTime" column="applyTime"></property>
		<property name="applicant" column="applicant"></property>
		<property name="state" column="state"></property>
	</class>
</hibernate-mapping>