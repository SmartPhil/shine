<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shine.dto.CallBackInfo" table="callbackinfo">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="followTime" column="dtFollowTime"></property>
		<property name="follower" column="sFollower"></property>
		<property name="followContent" column="sFollowContent"></property>
		<property name="oppId" column="nOppId"></property>
	</class>
</hibernate-mapping>