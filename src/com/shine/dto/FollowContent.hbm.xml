<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shine.dto.FollowContent" table="followcontent">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="oppId" column="oppId"></property>
		<property name="time" column="dtFollowTime"></property>
		<property name="content" column="sFollowContent"></property>
		<property name="follower" column="sFollower"></property>
	</class>
</hibernate-mapping>